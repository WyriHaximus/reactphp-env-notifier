{
  "name": "wyrihaximus/react-env-notifier",
  "description": "\ud83d\udce3 Notifies when an environment variable changes",
  "license": "MIT",
  "authors": [
    {
      "name": "Cees-Jan Kiewiet",
      "email": "ceesjank@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "react/event-loop": "^1.3",
    "reactivex/rxphp": "^2.0"
  },
  "require-dev": {
    "wyrihaximus/async-test-utilities": "^5.0.23"
  },
  "autoload": {
    "psr-4": {
      "WyriHaximus\\React\\Env\\Notifier\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WyriHaximus\\Tests\\React\\Env\\Notifier\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "ergebnis/composer-normalize": true,
      "icanhazstring/composer-unused": true,
      "infection/extension-installer": true,
      "composer-unused/composer-unused-plugin": true
    },
    "platform": {
      "php": "8.1.13"
    },
    "sort-packages": true
  },
  "scripts": {
    "post-install-cmd": [
      "composer normalize"
    ],
    "post-update-cmd": [
      "composer normalize"
    ]
  }
}
